<#
Find sAMAccountName based off users first and last name.
Single Column "User" in Excel with "First Last" convention
Ross Edwardson @ CMI/CORA | 3.5.2021
#>

#Variables

$UserList = Import-Csv 'C:\AD\List2.csv'
$OutFile = "C:\AD\export1.csv"

#Start Script

#Get Account Name
foreach ($User in $UserList){
    $domainname = $user.user
Get-ADUser -Filter{displayName -like $domainname} | select samAccountName | Export-Csv -Path '$OutFile' -NoType -Append }
